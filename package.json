{
   "name": "@silver886/amazon-cognito-saml",
   "version": "0.1.0",
   "description": "SAML IdP adaptor for Amazon Cognito",
   "main": "./dist/index.js",
   "types": "./dist/index.d.ts",
   "scripts": {
      "comments": "leasot --ignore .git/**,node_modules/** --tags NOTE OPTIMIZE TODO HACK XXX FIXME BUG --filetype .ts **",
      "format": "prettier --write . && eslint --fix .",
      "format-check": "prettier --check . && eslint .",
      "watch-test": "npm run test -- --watchAll",
      "test": "jest --coverage",
      "predev-start": "npm run tsoa-gen",
      "dev-start": "env NODE_ENV=local ts-node src/index.ts",
      "start": "node dist/index.js",
      "prebuild": "npm run tsoa-gen",
      "build": "tsc",
      "prebundle": "npm run tsoa-gen",
      "bundle": "webpack",
      "prepublish": "npm run build",
      "prepublishOnly": "npm run test",
      "tsoa-gen": "tsoa spec-and-routes"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/silver886/amazon-cognito-saml.git"
   },
   "keywords": [
      "aws",
      "cognito",
      "saml",
      "idp"
   ],
   "author": {
      "name": "Leo Liu",
      "email": "karta1041337@gmail.com"
   },
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/silver886/amazon-cognito-saml/issues"
   },
   "homepage": "https://github.com/silver886/amazon-cognito-saml#readme",
   "devDependencies": {
      "@silver886/eslint-config": "^2.2.4",
      "@tsoa/cli": "^5.1.1",
      "@types/body-parser": "^1.19.2",
      "@types/compression": "^1.7.2",
      "@types/cookie-parser": "^1.4.3",
      "@types/cors": "^2.8.13",
      "@types/express": "^4.17.17",
      "@types/jest": "^29.5.0",
      "@types/mime-types": "^2.1.1",
      "@types/node": "^18.15.11",
      "@types/swagger-ui-dist": "^3.30.1",
      "@types/swagger-ui-express": "^4.1.3",
      "@types/webpack": "^5.28.1",
      "@types/zip-webpack-plugin": "^3.0.3",
      "@typescript-eslint/eslint-plugin": "^5.57.0",
      "@typescript-eslint/parser": "^5.57.0",
      "copy-webpack-plugin": "^11.0.0",
      "eslint": "^8.37.0",
      "eslint-config-prettier": "^8.8.0",
      "eslint-import-resolver-node": "^0.3.7",
      "eslint-import-resolver-typescript": "^3.5.4",
      "eslint-plugin-import": "^2.27.5",
      "eslint-plugin-prettier": "^4.2.1",
      "jest": "^29.5.0",
      "jest-junit": "^15.0.0",
      "leasot": "^13.3.0",
      "prettier": "^2.8.7",
      "prettier-plugin-multiline-arrays": "^1.1.3",
      "ts-jest": "^29.0.5",
      "ts-node": "^10.9.1",
      "tsconfig-paths": "^4.2.0",
      "tsconfig-paths-webpack-plugin": "^4.0.1",
      "typescript": "^5.0.2",
      "webpack": "^5.77.0",
      "webpack-cli": "^5.0.1",
      "zip-webpack-plugin": "^4.0.1"
   },
   "dependencies": {
      "@authenio/samlify-node-xmllint": "^2.0.0",
